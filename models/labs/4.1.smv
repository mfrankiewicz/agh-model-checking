MODULE main
VAR
    s : {s0, s1, s2, s3};
    a : boolean;
    b : boolean;
ASSIGN
    init(s) := s0;
    next(s) := case
        s = s0 : {s1, s2};
        s = s1 : s3;
        s = s2 : s3;
        s = s3 : s2;
    esac;
    a := case
        s = s1 : FALSE;
        TRUE : TRUE;
    esac;
    b := case
        s = s3 : TRUE;
        TRUE : FALSE;
    esac;
