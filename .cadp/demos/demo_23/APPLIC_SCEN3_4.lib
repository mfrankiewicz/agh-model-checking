
(* ------------------------------------------------------------------------- *)

process Application [TDreq]
    		    (n: Nat, id: Nat)
                    : noexit
:=
   [id eq 0] ->
	(choice h: HEADER, d: DATA []
	(* k = 2 *)
          TDreq !id !n !h !d; 
          TDreq !id !n !h !d; 
          TDreq !id !n !h !d; 
          TDreq !id !n !h !d; 
	  stop
	)
   []
   [id ne 0] -> stop
endproc

