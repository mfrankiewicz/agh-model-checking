module LINK_5 (DATA) is

process LINK [INPUT, OUTPUT:PORT] is
   loop
      select
         INPUT (TOKEN);
         OUTPUT (TOKEN)
      []
         INPUT (TOKEN)
      []
         var Ai:ADDR in
            INPUT (CLAIM, ?Ai);
            OUTPUT (CLAIM, Ai)
         end var
      []
         INPUT (CLAIM, ?any ADDR)
      end select
   end loop
end process

end module

