(*
 * This library is used by the lnt2lotos translator
 *
 * It is built upon several predefined libraries of CADP (X_ACTION, X_BOOLEAN,
 * X_NATURAL, X_INTEGER, X_REAL, X_CHARACTER, and X_STRING), which are
 * enriched by adding more operators.
 *
 * Note: all binary infix operators must also be declared as binary prefix
 * operators so as to respect lnt2lotos conventions.
 *)

(* ------------------------------------------------------------------------- *)

library X_ACTION endlib

(* ------------------------------------------------------------------------- *)

library X_BOOLEAN endlib

type BooleanExtensions is Boolean
   opns
      _==_ (*! implementedby ADT_EQ_BIS_BOOL external *),
      _<>_ (*! implementedby ADT_NE_BIS_BOOL external *),
      _/=_ (*! implementedby ADT_NE_TER_BOOL external *),
      _lt_ (*! implementedby ADT_LT_BOOL external *),
      _<_  (*! implementedby ADT_LT_BIS_BOOL external *),
      _le_ (*! implementedby ADT_LE_BOOL external *),
      _<=_ (*! implementedby ADT_LE_BIS_BOOL external *),
      _gt_ (*! implementedby ADT_GT_BOOL external *),
      _>_  (*! implementedby ADT_GT_BIS_BOOL external *),
      _ge_ (*! implementedby ADT_GE_BOOL external *),
      _>=_ (*! implementedby ADT_GE_BIS_BOOL external *) : Bool, Bool -> Bool

      (* binary prefix operators *)
      and      (*! implementedby ADT_AND_BIS external *),
      and_then (*! implementedby ADT_AND_THEN_BIS external *),
      or       (*! implementedby ADT_OR_BIS external *),
      or_else  (*! implementedby ADT_OR_ELSE_BIS external *),
      xor      (*! implementedby ADT_XOR_BIS external *),
      implies  (*! implementedby ADT_IMPLIES_BIS external *),
      iff      (*! implementedby ADT_IFF_BIS external *),
      eq (*! implementedby ADT_EQ_TER_BOOL external *),
      == (*! implementedby ADT_EQ_QUA_BOOL external *),
      ne (*! implementedby ADT_NE_QUA_BOOL external *),
      <> (*! implementedby ADT_NE_QUI_BOOL external *),
      /= (*! implementedby ADT_NE_SEX_BOOL external *),
      lt (*! implementedby ADT_LT_TER_BOOL external *),
      <  (*! implementedby ADT_LT_QUA_BOOL external *),
      le (*! implementedby ADT_LE_TER_BOOL external *),
      <= (*! implementedby ADT_LE_QUA_BOOL external *),
      gt (*! implementedby ADT_GT_TER_BOOL external *),
      >  (*! implementedby ADT_GT_QUA_BOOL external *),
      ge (*! implementedby ADT_GE_TER_BOOL external *),
      >= (*! implementedby ADT_GE_QUA_BOOL external *) : Bool, Bool -> Bool
endtype

